<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="NS_Message">

    <!--  -->
    <select id="selMessageList" parameterType="map" resultType="camelMap">
        /* NS_Home.selMessageList */
        SELECT CODE
             , LOCALE
             , MESSAGE
             , LANGUAGE
             , COUNTRY
          FROM TYM_MESSAGE
    </select>

</mapper>